@model DAS.Backoffice.Models.Application.ApplicationForm
<script type="text/javascript">
    function submitApplicationForm() {
        var $form = $('#form-application');
        if ($form.valid()) {
            showProgressIndicator('form-application');
            ini.ajax.post('@(Url.Action("SubmitApplication","Home"))', ini.ajax.contentType.UrlEncoded,
                $form.serialize(),
                function (result) {
                    hideProgressIndicator();
                    showAlert('Message', result);
                },
                function (error) {
                    hideProgressIndicator();
                    showAlert('Error', error);
                });
        }
        else showErrorTooltip($form);
    }

    //$('body').on('keypress', function (event) {
    //    if (event.keyCode == 13) {
    //        $("#btn-search-App").click();
    //    }
    //});

    if (document.getElementById('ActivitiesOnClientsSites').value != "" && document.getElementById('ActivitiesOnClientsSites').value !=null ){
        document.getElementById('yesCheck').checked = true;
        document.getElementById('ifYes').style.display = 'block';
       

  }

  
</script>
